{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Default": "Host=localhost;Username=nakul;Password=password;Database=jailtalk;Port=5433"
  },
  "JwtSettings": {
    "Issuer": "asd",
    "Audience": "asd",
    "Key": "asd",
    "SessionVerificationKey": "asd",
    "ValidateFaceImage": false
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/app.log",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level}] {Message}{NewLine}{Exception}"
        }
      }
    ]
  },
  "Aws": {
    "Rekognition": {
      "UseBasicCredentialAuthentication": false,
      "AccessKey": "accessKey",
      "SecretKey": "secretKey",
      "Region": "region"
    },
    "S3": {
      "AccessKey": "accessKey",
      "SecretKey": "secretKey",
      "Region": "region",
      "UseBasicCredentialAuthentication": true,
      "BucketName": "jail-connect"
    }
  },
  "AccountManager": {
    "RechargeSecret":  "asd"
  }
}
